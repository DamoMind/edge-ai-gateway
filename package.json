{
  "name": "edge-ai-gateway",
  "version": "0.1.0",
  "description": "Lightweight AI provider abstraction layer for edge deployment (Cloudflare Workers)",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./providers/*": {
      "types": "./dist/providers/*.d.ts",
      "import": "./dist/providers/*.mjs",
      "require": "./dist/providers/*.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai",
    "llm",
    "openai",
    "azure",
    "cloudflare",
    "edge",
    "workers",
    "gateway",
    "proxy"
  ],
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "wrangler": "^4.63.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
